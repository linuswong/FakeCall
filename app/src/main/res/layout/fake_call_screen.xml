<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#070707"
    android:backgroundTint="#921E1E1E"
    android:foregroundTint="#FFFFFF"
    tools:context=".FakeCallScreen">>

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingActionButton_keypad_fakeCallScreen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:clickable="true"
        android:elevation="0dp"
        android:tint="#FAEEEE"
        app:backgroundTint="#26191010"
        app:elevation="0dp"
        app:layout_constraintBottom_toTopOf="@+id/guideline2"
        app:layout_constraintEnd_toStartOf="@+id/floatingActionButton_speaker_fakeCallScreen"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/floatingActionButton_mute_fakeCallScreen"
        app:srcCompat="@drawable/ic_baseline_dialpad_24"
        app:tint="@null" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingActionButton_mute_fakeCallScreen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:clickable="true"
        android:elevation="0dp"
        android:tint="#FAEEEE"
        app:backgroundTint="#26191010"
        app:elevation="0dp"
        app:layout_constraintBottom_toTopOf="@+id/guideline2"
        app:layout_constraintEnd_toStartOf="@+id/floatingActionButton_keypad_fakeCallScreen"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@android:drawable/stat_notify_call_mute"
        app:tint="@null" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingActionButton_speaker_fakeCallScreen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:clickable="true"
        android:elevation="0dp"
        android:tint="#FAEEEE"
        app:backgroundTint="#26191010"
        app:elevation="0dp"
        app:layout_constraintBottom_toTopOf="@+id/guideline2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/floatingActionButton_keypad_fakeCallScreen"
        app:srcCompat="@drawable/ic_baseline_volume_up_24"
        app:tint="@null" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingActionButton_addCaller_fakeCallScreen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:clickable="true"
        android:elevation="0dp"
        android:tint="#FAEEEE"
        app:backgroundTint="#26191010"
        app:elevation="0dp"
        app:layout_constraintEnd_toStartOf="@+id/floatingActionButton_videoCall_fakeCallScreen"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline2"
        app:srcCompat="@drawable/ic_baseline_add_24"
        app:tint="@null" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingActionButton_videoCall_fakeCallScreen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:clickable="true"
        android:elevation="0dp"
        android:tint="#FAEEEE"
        app:backgroundTint="#26191010"
        app:elevation="0dp"
        app:layout_constraintEnd_toStartOf="@+id/floatingActionButton_contacts_fakeCallScreen"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/floatingActionButton_addCaller_fakeCallScreen"
        app:layout_constraintTop_toTopOf="@+id/guideline2"
        app:srcCompat="@drawable/ic_baseline_videocam_24"
        app:tint="@null" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingActionButton_contacts_fakeCallScreen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:clickable="true"
        android:elevation="0dp"
        android:tint="#FFFFFF"
        app:backgroundTint="#26191010"
        app:elevation="0dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/floatingActionButton_videoCall_fakeCallScreen"
        app:layout_constraintTop_toTopOf="@+id/guideline2"
        app:srcCompat="@drawable/ic_baseline_contacts_24"
        app:tint="@null" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.50" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="mute"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="@+id/floatingActionButton_mute_fakeCallScreen"
        app:layout_constraintStart_toStartOf="@+id/floatingActionButton_mute_fakeCallScreen"
        app:layout_constraintTop_toBottomOf="@+id/floatingActionButton_mute_fakeCallScreen" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="keypad"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="@+id/floatingActionButton_keypad_fakeCallScreen"
        app:layout_constraintStart_toStartOf="@+id/floatingActionButton_keypad_fakeCallScreen"
        app:layout_constraintTop_toBottomOf="@+id/floatingActionButton_keypad_fakeCallScreen"
        tools:text="keypad" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="speaker"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="@+id/floatingActionButton_speaker_fakeCallScreen"
        app:layout_constraintStart_toStartOf="@+id/floatingActionButton_speaker_fakeCallScreen"
        app:layout_constraintTop_toBottomOf="@+id/floatingActionButton_speaker_fakeCallScreen" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="add call"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="@+id/floatingActionButton_addCaller_fakeCallScreen"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/floatingActionButton_addCaller_fakeCallScreen"
        app:layout_constraintTop_toBottomOf="@+id/floatingActionButton_addCaller_fakeCallScreen" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="video call"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="@+id/floatingActionButton_videoCall_fakeCallScreen"
        app:layout_constraintStart_toStartOf="@+id/floatingActionButton_videoCall_fakeCallScreen"
        app:layout_constraintTop_toBottomOf="@+id/floatingActionButton_videoCall_fakeCallScreen" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="contacts"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="@+id/floatingActionButton_contacts_fakeCallScreen"
        app:layout_constraintStart_toStartOf="@+id/floatingActionButton_contacts_fakeCallScreen"
        app:layout_constraintTop_toBottomOf="@+id/floatingActionButton_contacts_fakeCallScreen" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingActionButton_hangUp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#FF0000"
        android:clickable="true"
        android:elevation="0dp"
        android:tint="#FAEEEE"
        app:elevation="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:srcCompat="@drawable/ic_baseline_call_end_24"
        app:tint="@null" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="80dp"
        android:text="Jonathaosn MAis"
        android:textColor="@color/white"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/floatingActionButton_keypad_fakeCallScreen"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent" />

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/imageView"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_margin="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:shapeAppearanceOverlay="@style/ImageView.Circle"
        app:srcCompat="@drawable/profile_picture" />

</androidx.constraintlayout.widget.ConstraintLayout>